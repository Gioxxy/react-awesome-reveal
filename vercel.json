{
  "name": "react-awesome-reveal",
  "version": 2,
  "build": {
    "env": {
      "GTM_ID": "@gtm-react-awesome-reveal"
    }
  },
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@now/static-build"
    },
    {
      "src": "website/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    { "src": "/next/(.*)", "dest": "/website/$1" },
    { "src": "/(.*)", "dest": "/docs/$1" }
  ]
}
